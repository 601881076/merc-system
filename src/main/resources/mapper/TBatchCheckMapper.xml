<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhnj.mapper.TBatchCheckMapper">
    <sql id="batchCheckSql">
        id,
        batch_id,
        type,
        check_result,
        up_user_id,
        up_user_name,
        check_user_id,
        check_user_name,
        remark,
        create_time,
        check_time,
        update_time
    </sql>
    <insert id="insert" parameterType="list">
        insert into t_batch_check
            (batch_id,up_user_id,up_user_name,check_user_id,check_user_name,remark)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (
             #{item.batchId,jdbcType=BIGINT},
             #{item.upUserId,jdbcType=BIGINT},
             #{item.upUserName,jdbcType=VARCHAR},
             #{item.checkUserId,jdbcType=BIGINT},
             #{item.checkUserName,jdbcType=VARCHAR},
             #{item.remark,jdbcType=VARCHAR}
            )
        </foreach>
    </insert>
</mapper>